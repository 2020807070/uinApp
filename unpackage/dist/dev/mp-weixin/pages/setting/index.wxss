
.setting {
	height: 94vh;
	background-color: rgb(238, 238, 238);
	overflow: hidden;
}
.setting-btn {
	text-align: center;
	line-height: 80rpx;
	letter-spacing: 5rpx;
	color: rgba(0, 0, 0, .8);
	background-color: #FFFFFF;
	width: 100%;
	margin-top: 30rpx;
	font-size: 30rpx;
	height: 80rpx;
}
.setting-mian {
	overflow: hidden;
	margin-top: 30rpx;
}
.setting-item {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	background-color: #FFFFFF;
	border-bottom: 1rpx solid rgba(0, 0, 0, .1);
	padding: 26rpx 30rpx;
	font-size: 26rpx;
	color: rgba(0, 0, 0, .7);
	padding-right: 50rpx;
}
.setting-item .icon {
	position: absolute;
	right: 15rpx;
	width: 17rpx;
	height: 32rpx;
}
.head {
	width: 80rpx;
	height: 80rpx;
}

